{
  "name": "artmo",
  "version": "2.0.0",
  "scripts": {
    "build": "rm -rf ./dist/* && npm run build-ts && npm run lint && npm run templates",
    "build-ts": "tsc",
    "lint": "eslint . --ext .ts",
    "start": "node -r module-alias/register ./dist/src/ --env=development",
    "dev": "npm run lint && nodemon",
    "test": "nodemon --config ./spec/nodemon.json",
    "templates": "cp -R ./src/templates ./dist/src/"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register --files ./src"
  },
  "_moduleAliases": {
    "@schema": "dist/src/models",
    "@utilities": "dist/src/utilities",
    "@server": "dist/src/Server",
    "@controller": "dist/src/controllers"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "5.3.1",
    "helmet": "^3.23.3",
    "http-status-codes": "^1.4.0",
    "i18n": "^0.8.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.11.8",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "qs": "^6.9.4",
    "randomatic": "^3.1.1",
    "tslint-config-security": "^1.16.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.47",
    "@types/i18n": "^0.8.8",
    "@types/jasmine": "^3.6.2",
    "@types/jsonfile": "^6.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.165",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.13",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/randomatic": "^3.1.0",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.6.3",
    "jsonfile": "^6.1.0",
    "nodemon": "^2.0.6",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}
